{
  "name": "sls-plugin-dynamodb-offline",
  "version": "1.0.0",
  "description": "Serverless Framework Plugin to Work with AWS DynamoDB Offline",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "tsc",
    "watch": "tsc -w",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "start": "npm link && nodemon",
    "make_bin": "mkdir -p bin",
    "download": "curl -o bin/dynamodb_local_latest.tar.gz https://s3-us-west-2.amazonaws.com/dynamodb-local/dynamodb_local_latest.tar.gz",
    "downloadSHA": "curl -o bin/dynamodb_local_latest.tar.gz.sha256 https://s3-us-west-2.amazonaws.com/dynamodb-local/dynamodb_local_latest.tar.gz.sha256",
    "checkSHA": "cd bin && shasum -c dynamodb_local_latest.tar.gz.sha256",
    "extract_archive": "tar -xzf bin/dynamodb_local_latest.tar.gz -C bin",
    "postinstall": "npm run make_bin && cd bin && npm run downloadSHA && npm run checkSHA || npm run download && npm run extract_archive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/93v/sls-plugin-dynamodb-offline.git"
  },
  "keywords": [
    "serverless",
    "serverless-plugin",
    "sls",
    "sls-plugin",
    "aws",
    "dynamodb",
    "local",
    "offline",
    "dynamodb local",
    "dynamodb offline"
  ],
  "author": {
    "name": "93v",
    "email": "93v@protonmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/93v/sls-plugin-dynamodb-offline/issues"
  },
  "homepage": "https://github.com/93v/sls-plugin-dynamodb-offline#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "@types/serverless": "^1.18.2",
    "eslint": "^6.1.0",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "aws-sdk": "^2.506.0",
    "serverless": "^1.49.0",
    "ts-optchain": "^0.1.8"
  }
}
